@model List<DepartmentEmployeeReadVM>


<table class="table table-bordered">
    <thead>
        <tr>
            <th>
                Employee Id
            </th>
            <th>
                Employee Name
            </th>
            <th>
                Age
            </th>
            <th>
                Salary
            </th>
            <th>
                Department
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>
                    @employee.Id
                </td>
                <td>
                    @employee.Name
                </td>
                <td>
                    @employee.Age
                </td>
                <td>
                    @employee.Salary
                </td>
                <td>
                    @employee.Department
                </td>
            </tr>
        }
    </tbody>
</table>
